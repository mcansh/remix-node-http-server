{
  "name": "@mcansh/remix-raw-http",
  "version": "1.0.1",
  "description": "Node HTTP server request handler for Remix using http.createServer",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "module": "./dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": {
        "require": "./dist/index.d.cts",
        "import": "./dist/index.d.ts",
        "default": "./dist/index.d.cts"
      },
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "repository": {
    "url": "mcansh/remix-node-http-server",
    "directory": "packages/remix-raw-http",
    "type": "git"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/mcansh"
    }
  ],
  "keywords": [
    "remix",
    "remix-run",
    "node",
    "http",
    "server",
    "request",
    "handler"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsup && publint",
    "test": "vitest"
  },
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh/)",
  "license": "MIT",
  "devDependencies": {
    "@fastify/send": "^2.1.0",
    "@remix-run/node": "^2.5.0",
    "@types/supertest": "^6.0.2",
    "node-mocks-http": "^1.14.1",
    "supertest": "^6.3.3"
  },
  "peerDependencies": {
    "@remix-run/node": "^2.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
